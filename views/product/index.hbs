{{# each products }}
	<div class="row">
		{{# each this }}
			<div class="col-xs-6 col-sm-4 col-lg-3">
				<div class="thumbnail">
				  	<img src="{{ this.image }}" alt="Product image" class ="img-responsive">
				  	<div class="caption">
				  		<h5 class="card-title">{{ this.name }}</h5>
				    	<ul class="list-unstyled">
							<li>Category: {{ this.category }}</li>
							<li>Description: {{ this.description }}</li>
							<li>Timer: {{ this.timer }}</li>  
							<li>Minimum Bid: {{ this.amount }}</li>
							<li>Seller: {{ this.seller }}</li>
					    </ul>
					    <div class="clearfix">
				    		<a href="#" class="btn btn-primary">Bid</a>
				    	</div>
				  	</div>
				</div>	
			</div>
		{{/each}}
	</div> 
{{/each}}