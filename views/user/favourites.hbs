<table class="table table-hover">
  <thead>
   <tr>
     <th scope="col">image</th>
     <th scope="col">details</th>
   </tr>
 </thead>
 {{# each products }}
 <tr>
   <!-- <th scope="row">1</th> -->
   <td>image</td>
   <td>
     <ul class="list-unstyled">
       <li class="font-weight-bold">{{ this.name }}</li>
       <li>Category: {{ this.category }}</li>
       <li>Description: {{ this.description }}</li>
       <li>Timer: {{ this.timer }}</li>
       <li>Minimum Bid: {{ this.amount }}</li>
       <li>Seller: {{ this.seller }}</li>
       <!-- <li>Buyer: {{this.buyer}} </li> -->
     </ul>
   </td>
 </tr>
 {{/each}}
</table>
